<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<bean id="fileUploadBean" 		class= "www.com.screenplay.awards.fileupload.ScreenPlayFileUploadImpl" 				/>
	<bean id="payAndUploadBo" 		class= "www.com.screenplay.awards.bo.PayAndUploadBo" 								/>
		<!--property name="fileUpload" 	ref="fileUploadBean" />
	</bean-->
	<bean id="payAndUploadService" 	class="www.com.screenplay.awards.service.implementation.PayAndUploadServiceImpl" init-method="initialize" >
		<property name="payAndUploadBusinessObject" ref="payAndUploadBo"/>
		<property name="screanPlayFileUpload" ref="fileUploadBean"/>
	</bean>
	
	
	<bean id="laSpaPayAndUploadBackingBean" class="www.com.screenplay.awards.backingbean.LaSpaPayAndUloadBackingBeanImpl"  scope="request">
		<property name="payAndUploadService" ref="payAndUploadService" />
		<property name="eventPublisher" ref="eventPublisher" />
		<!--property name="payAndUploadBusinessObject" ref="payAndUploadBo"/>
		<property name="screanPlayFileUpload" ref="fileUploadBean"/-->
	</bean>
	<!--bean id="selectItemToStringConverter" class="www.com.screenplay.awards.converter.SelectItemToStringConverter"/-->
</beans>